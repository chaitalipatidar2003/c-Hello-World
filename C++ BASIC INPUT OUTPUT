#include<iostream>
using namespace std;

int main()
{
    int num1,num2;
    cout<<"enter the value of num 1:\n";/*'<<'is a insertion operator*/
    cin>>num1;/*'>>'is an extraction operator*/

    cout<<"enter the value of num 2:\n";/*'<<'is a insertion operator*/
    cin>>num2;/*'>>'is an extraction operator*/
    cout<<"The sum is "<<num1 + num2;

    /* code */
    return 0;
}
